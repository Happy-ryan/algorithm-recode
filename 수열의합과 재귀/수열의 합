def summation(n) :
    s = 0
    for i in range(1, n+1) :
        s += i*(n+1-i)
    return s

print(summation(20))

# 1*20 + 2*19 + ... 19*2+20*1 

print("============================1")

numbers=[36,59,66,38,78,97,25,41,29,12]
print(sum(numbers)) #numbers리스트 요소들의 합계를 알려주는 내장함수

s = 0
for n in numbers :
    s += n
print(s)

print("============================2")
